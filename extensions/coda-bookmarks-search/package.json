{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "coda-bookmarks-search",
  "title": "Coda Bookmarks Search",
  "description": "Quickly search a coda bookmarks table and open a URL in your browser.",
  "icon": "command-icon.png",
  "author": "allienx",
  "categories": [
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Coda Bookmarks",
      "description": "Quickly search a coda bookmarks table and open a URL in your browser.",
      "mode": "view"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "dependencies": {
    "@raycast/api": "1.36.0",
    "axios": "0.27.2",
    "node-fetch": "3.2.6"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "17.0.45",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  }
}
